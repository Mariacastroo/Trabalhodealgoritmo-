public class ImprimirVetor {
    public static void main(String[] args) {
        int[] vetor = {1, 2, 3, 4, 5};

        imprimirVetor(vetor);
    }

    // Função para imprimir um vetor na tela
    static void imprimirVetor(int[] vetor) {
        for (int i = 0; i < vetor.length; i++) {
            System.out.print(vetor[i] + " ");
        }
        System.out.println();
    }
}
